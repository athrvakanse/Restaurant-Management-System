{
  "spring": {
    "application": {
      "name": "waiter_srv" // mention your app name . Dont use _ in name.
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/", //Eureka service
      "shouldRegisterWithEureka": true,
      "shouldFetchRegistry": false
    },
    "instance": {
      "port": 5046 // mention your port no of http url
    }
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}